<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <context:annotation-config/>
    <import resource="classpath:dubbo/dubbo-provider.xml"/>
    <bean id="jiPushUtil" class="com.shengsu.util.JiPushUtil">
        <constructor-arg name="masterSecret" value="${jiguang.masterSecret}"/>
        <constructor-arg name="appKey" value="${jiguang.appKey}"/>
        <constructor-arg name="apnsProduction" value="${jiguang.apnsProduction}"/>
    </bean>
    <bean id="JpushNormalServiceImpl" class="com.shengsu.helper.service.impl.JpushNormalServiceImpl"/>
    <bean id="JpushScheduleServiceImpl" class="com.shengsu.helper.service.impl.JpushScheduleServiceImpl"/>
    <bean id="JpushScheduleCancelServiceImpl" class="com.shengsu.helper.service.impl.JpushScheduleCancelServiceImpl"/>
</beans>